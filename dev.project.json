{
	"name": "replace-dev",
	"globIgnorePaths": ["**/tsconfig.json", "**/package.json", "**/package-lock.json"],

	"tree": {
		"$className": "DataModel",

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",

			"node_modules": {
				"$className": "Folder",

				"@rbxts": {
					"$path": "node_modules/@rbxts"
				}
			},

			"replace": {
				"$path": "out"
			},

			"include": {
				"$path": "include"
			}
		}
	}
}